<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
<!-- 默认是矩形-->
    <!-- 指定形状内部的填充颜色 -->
    <solid android:color="#CFC6A6"/>
    <!-- 指定形状轮廓的粗细与颜色 -->
    <stroke android:color="#F36E6E" android:width="1dp" android:dashWidth="10dp"/>
    <!-- 指定圆角的半径，没有corners属性则表示没有圆角 -->
    <corners android:radius="50dp"/>
    <!--指定形状内部颜色渐变-->
    <gradient android:angle="90" android:type="linear" android:startColor="@color/teal_200" android:endColor="@color/teal_700"/>

</shape>